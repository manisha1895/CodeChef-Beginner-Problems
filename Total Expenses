#include<iostream>
#include<iomanip>
using namespace std;
int main(){
  int T,i;
  float q,p; //q is quantity , p is price per item
  cin>>T;
  for(i=0;i<T;i++){
    cin>>q>>p;
    cout<<fixed;
    cout<<setprecision(6);
    if(q>1000){
    	
    	cout<<0.9*q*p<<endl;
    }
    else cout<<q*p<<endl;
	 
  }  
  return 0;
  
}  
