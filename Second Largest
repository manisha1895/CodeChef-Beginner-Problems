#include<iostream>
using namespace std;
int main(){
  int T,i,a,b,c,sec_large,max,min;
  cin>>T;
  for(i=0;i<T;i++){
  	cin>>a>>b>>c;
  	sec_large = a+b+c;
  	max=a;
  	min=a;
  	if(b>max) max=b;
  	if(c>max) max=c;
  	if(b<min) min=b;
  	if(c<min) min=c;
  	sec_large -= (max+min);
  	cout<<sec_large<<endl;
  	
  }  
  return 0;
  
}  
