#include<iostream>
#include<math.h>
using namespace std;
int main(){
  int T,i,N,j,flag;
  cin>>T;
  for(i=0;i<T;i++){
    cin>>N;
    flag=0;
    for(j=2;j<=(int)sqrt(N);j++){
    	if(N%j==0) {
    		flag=1;
    		break;
    	}
    }
    if(flag) cout<<"no"<<endl;
	else cout<<"yes"<<endl;
	 
  }  
  return 0;
  
}  
